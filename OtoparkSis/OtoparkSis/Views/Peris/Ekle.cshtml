@model OtoparkSis.Models.Entity.Tbl_Personel
@{
    ViewBag.Title = "Ekle";
    Layout = "~/Views/Shared/_Yonetici.cshtml";
}

<h2>Personel Kayıt</h2>

<form method="post">
    <label>Personel Adı Soyadı</label>
    @Html.TextBoxFor(x => x.Ad, new { @class = "form-control" })
    <br />
    <label>Personel Kullanıcı Adı</label>
    @Html.TextBoxFor(x => x.KullaniciAdi, new { @class = "form-control" })
    <br />
    <label>Personel Şifresi</label>
    @Html.TextBoxFor(x => x.Sifre, new { @class = "form-control" })
    <br />
    <label>Personel Telefon Numarası</label>
    @Html.TextBoxFor(x => x.Telefon, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Ekle</button>
</form>